<Window x:Class="UserPermissions.UserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UserPermissions"
        mc:Ignorable="d"
        Title="UserWindow" Height="450" Width="800">
    
    <Grid>
        <StackPanel Orientation="Horizontal" Height="35" VerticalAlignment="Top" HorizontalAlignment="Center">
            <Button Content="Create user" MinWidth="100" Click="CreateUserBtn"/>
            <Button Content="Delete user" MinWidth="100" Click="DeleteUserBtn" Margin="10,0,0,0"/>
        </StackPanel>
        
        <Frame x:Name="UserWin" Margin="0,35,0,0" NavigationUIVisibility="Hidden" />

    </Grid>
</Window>
